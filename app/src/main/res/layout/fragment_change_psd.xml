<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>
        <variable
            name="data"
            type="com.example.yilaoapp.ui.mine.MineViewModel" />
    </data>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.mine.ChangePsdFragment">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <View
                android:id="@+id/view"
                android:layout_width="wrap_content"
                android:layout_height="2dp"
                app:layout_constraintBottom_toBottomOf="@+id/toolbar"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/toolbar"
                android:background="@color/div_white" />

            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbar"
                android:layout_width="match_parent"
                android:layout_height="?attr/actionBarSize"
                android:background="?attr/colorPrimary"
                android:theme="@style/ThemeOverlay.AppCompat.ActionBar"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:popupTheme="@style/ThemeOverlay.AppCompat.Light">

            </androidx.appcompat.widget.Toolbar>

            <TextView
                android:id="@+id/biaoti"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="@string/Chage_psd"
                android:textColor="#000000"
                android:textSize="18sp"
                app:layout_constraintBottom_toBottomOf="@+id/toolbar"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/toolbar" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/change_psd_guideline110"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.18" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/change_psd_guideline111"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.28" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/change_psd_guideline112"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.38" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/change_psd_guideline113"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.48" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/change_psd_guideline114"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.7" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/change_psd_guideline115"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.1" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/change_psd_guideline"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.33" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/change_psd_guideline3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.38" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/change_psd_guideline4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.62" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/change_psd_guideline2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.81" />

            <TextView
                android:id="@+id/change_psd_textView44"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/oldPsd"
                android:textColor="@color/text_color"
                android:textSize="18sp"
                app:layout_constraintBottom_toTopOf="@+id/change_psd_guideline111"
                app:layout_constraintEnd_toStartOf="@+id/change_psd_guideline"
                app:layout_constraintStart_toStartOf="@+id/change_psd_guideline115"
                app:layout_constraintTop_toTopOf="@+id/change_psd_guideline110" />

            <TextView
                android:id="@+id/change_psd_textView45"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/NewPsd"
                android:textColor="@color/text_color"
                android:textSize="18sp"
                app:layout_constraintBottom_toTopOf="@+id/change_psd_guideline112"
                app:layout_constraintEnd_toStartOf="@+id/change_psd_guideline"
                app:layout_constraintStart_toStartOf="@+id/change_psd_guideline115"
                app:layout_constraintTop_toTopOf="@+id/change_psd_guideline111" />

            <TextView
                android:id="@+id/change_psd_textView46"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/checkPsd"
                android:textColor="@color/text_color"
                android:textSize="18sp"
                app:layout_constraintBottom_toTopOf="@+id/change_psd_guideline113"
                app:layout_constraintEnd_toStartOf="@+id/change_psd_guideline"
                app:layout_constraintStart_toStartOf="@+id/change_psd_guideline115"
                app:layout_constraintTop_toTopOf="@+id/change_psd_guideline112" />

            <com.rengwuxian.materialedittext.MaterialEditText
                android:id="@+id/editText9"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:hint="@string/yuanmima"
                android:inputType="textPassword"
                android:textColor="@color/text_color"
                app:layout_constraintBottom_toTopOf="@+id/change_psd_guideline111"
                app:layout_constraintEnd_toStartOf="@+id/change_psd_guideline2"
                app:layout_constraintStart_toStartOf="@+id/change_psd_guideline"
                app:layout_constraintTop_toTopOf="@+id/change_psd_guideline110"
                tools:targetApi="o" />

            <com.rengwuxian.materialedittext.MaterialEditText
                android:id="@+id/change_psd_editText10"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:autofillHints=""
                android:ems="10"
                android:hint="@string/new_psd"
                android:inputType="textPassword"
                android:textColor="@color/text_color"
                app:layout_constraintBottom_toTopOf="@+id/change_psd_guideline112"
                app:layout_constraintEnd_toStartOf="@+id/change_psd_guideline2"
                app:layout_constraintStart_toStartOf="@+id/change_psd_guideline"
                app:layout_constraintTop_toTopOf="@+id/change_psd_guideline111"
                app:layout_constraintVertical_bias="0.428"
                tools:targetApi="o" />

            <com.rengwuxian.materialedittext.MaterialEditText
                android:id="@+id/change_psd_editText11"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:autofillHints=""
                android:ems="10"
                android:hint="@string/check_psd"
                android:inputType="textPassword"
                android:textColor="@color/text_color"
                app:layout_constraintBottom_toTopOf="@+id/change_psd_guideline113"
                app:layout_constraintEnd_toStartOf="@+id/change_psd_guideline2"
                app:layout_constraintStart_toStartOf="@+id/change_psd_guideline"
                app:layout_constraintTop_toTopOf="@+id/change_psd_guideline112"
                tools:targetApi="o" />

            <Button
                android:id="@+id/change_psd_button9"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:background="@drawable/button_deepen"
                android:text="@string/Determine"
                android:textColor="#FFFFFF"
                android:textSize="18sp"
                app:layout_constraintBottom_toTopOf="@+id/change_psd_guideline114"
                app:layout_constraintEnd_toStartOf="@+id/change_psd_guideline4"
                app:layout_constraintStart_toStartOf="@+id/change_psd_guideline3"
                app:layout_constraintTop_toTopOf="@+id/change_psd_guideline113" />


        </androidx.constraintlayout.widget.ConstraintLayout>
    </FrameLayout>
</layout>